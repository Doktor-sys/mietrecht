  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:security": "eslint src --ext .ts,.tsx --rulesdir ./eslint-security-rules",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check",
    "validate:security": "npm run lint:security && npm audit",
    "accessibility-test": "npm test -- --testNamePattern=\"accessibility\"",
    "visual-test": "npx playwright test --grep visual",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "clean": "rm -rf build coverage node_modules/.cache",
    "clean:build": "rm -rf build",
    "security:audit": "npm audit",
    "security:audit-fix": "npm audit fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001"
}
