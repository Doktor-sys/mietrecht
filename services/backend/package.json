{
  "name": "@smartlaw/backend",
  "version": "1.0.0",
  "description": "Backend Service f√ºr SmartLaw Mietrecht Agent",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "node scripts/run-tests.js --unit",
    "test:integration": "node scripts/run-tests.js --integration",
    "test:e2e": "node scripts/run-tests.js --e2e",
    "test:coverage": "node scripts/run-tests.js --coverage",
    "test:watch": "jest --watch",
    "test:all": "node scripts/run-tests.js --coverage",
    "test:mutation": "node scripts/run-mutation-tests.js",
    "test:parallel": "node scripts/run-tests.js --parallel",
    "test:ci": "node scripts/run-tests.js --coverage --parallel",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint:security": "eslint src --ext .ts --rulesdir ./eslint-security-rules",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check",
    "validate:ci": "npm run lint && npm run type-check && npm run test:ci",
    "validate:security": "npm run lint:security && npm audit",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:test:reset": "DATABASE_URL=$TEST_DATABASE_URL prisma migrate reset --force",
    "certs:generate": "node scripts/generate-dev-certs.js",
    "certs:info": "openssl x509 -in certs/server-cert.pem -noout -text",
    "kms:validate": "node scripts/validate-kms-config.js",
    "kms:generate-keys": "node scripts/generate-kms-keys.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit-high": "npm audit --audit-level=high",
    "security:audit-critical": "npm audit --audit-level=critical",
    "security:check": "npm run security:audit && npm run security:audit-high && npm run security:audit-critical",
    "security:audit-fix": "npm audit fix",
    "dependency:audit": "node ../../scripts/dependency-audit.js",
    "clean": "rm -rf dist coverage reports node_modules/.cache",
    "clean:build": "rm -rf dist",
    "start:pm2": "pm2 start ecosystem.config.js",
    "stop:pm2": "pm2 stop ecosystem.config.js",
    "restart:pm2": "pm2 restart ecosystem.config.js",
    "status:pm2": "pm2 status",
    "logs:pm2": "pm2 logs"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.8.1",
    "@prisma/client": "^5.1.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "clamscan": "^2.1.2",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "minio": "^7.1.3",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.4",
    "openai": "^3.3.0",
    "otplib": "^12.0.1",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.13.0",
    "pg": "^8.16.3",
    "prisma": "^5.1.0",
    "prom-client": "^14.2.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.7",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^14.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tesseract.js": "^5.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/jest-runner": "^9.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.8",
    "@types/papaparse": "^5.5.1",
    "@types/pdf-parse": "^1.1.1",
    "@types/pdfkit": "^0.13.1",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.8",
    "jest": "^29.6.1",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
